import java.util.Scanner;

public class Test47 {
  public static void main(String[] args) throws Exception {
    //int[] arr = {3, 6, 8, 4, 12, 1, 9, 7};
    int i = 2, j = 1;
    
    while (i <= 9) {
      j = 1;
      while(j <= 9) {
        System.out.printf("%d * %d = %d\n", i, j, i * j);
        j++;
      }
      i++;
    }
  }
}
