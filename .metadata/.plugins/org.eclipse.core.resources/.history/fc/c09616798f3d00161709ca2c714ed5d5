import java.util.Scanner;

public class Test47 {
  public static void main(String[] args) throws Exception {
    Scanner keyScanner = new Scanner(System.in);
    int num = Integer.parseInt(keyScanner.nextLine());
    int i = 0;
    while(i < (num / 2) + 1) {
      while(i < (num / 2)) {
        System.out.print(" ");
        i--;
      }
      i++;
      System.out.println();
    }
  }
}
