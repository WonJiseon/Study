package step08.ex04;


public class Exam058 {
  public static void main(String[] args) {
    System.out.printf("A.var1 = %d\n", A.var1);
    System.out.printf("B.var3 = %d\n", B.var3);
    System.out.println("----------------------");
    
    B.m();
    System.out.printf("A.var1 = %d\n", A.var1);
    System.out.printf("B.var3 = %d\n", B.var3);
  }
}
