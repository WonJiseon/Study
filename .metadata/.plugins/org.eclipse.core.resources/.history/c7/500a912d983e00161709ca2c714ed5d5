package step08.ex06;

public class Calc3 extends Calc{
  // 자바는 파라미터 타입이 다르거나 개수가 다르면 같은 이름을 사용할 수 있다.
  // => 물론 같은 기능을 수행하는 메서드끼리 같은 이름을 사용해야 한다.
  void plusArray(int[] arr) {
    for (int i = 0; i < arr.length; i++) {
      this.result += arr[i];
    }
  }
  
  void plus(int a, int b) {
    this.result += a;
    this.result += b;
  }
}
